{
  "name": "abp-antd",
  "version": "5.0.0",
  "author": {
    "email": "1002275364@qq.com",
    "name": "xiexingen"
  },
  "description": "Abp vNext Pro的 react antd实现版本",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env UMI_ENV=prod umi build",
    "docker:build": "docker-compose -f ./docker/docker-compose.yml build",
    "docker:push": "npm run docker-hub:build && npm run docker:tag && docker push wetrial/template",
    "docker:tag": "docker tag template wetrial/template",
    "fetch:blocks": "pro fetch-blocks --branch antd@4 && npm run prettier",
    "functions:build": "netlify-lambda build ./lambda",
    "functions:run": "cross-env NODE_ENV=dev netlify-lambda serve ./lambda",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "dev": "npm run start:dev",
    "start:dev": "cross-env UMI_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env UMI_ENV=pre umi dev",
    "start:test": "cross-env UMI_ENV=test MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "postinstall": "umi g tmp",
    "ui": "umi ui"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.less": "stylelint --syntax less",
    "./**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/pro-form": "^1.15.1",
    "@ant-design/pro-layout": "^6.12.0",
    "@wetrial/component": "^4.0.0-beta.4",
    "@wetrial/core": "^4.0.0-beta.4",
    "@wetrial/hooks": "^4.0.0-beta.4",
    "@wetrial/provider": "^4.0.0-beta.4",
    "ahooks": "^2.9.4",
    "antd": "^4.10.3",
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@umijs/fabric": "^2.5.6",
    "@umijs/plugin-esbuild": "^1.1.0",
    "@umijs/preset-react": "^1.7.13",
    "@umijs/test": "^3.3.7",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "husky": "^4.3.7",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.3",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "react-dev-inspector": "^1.1.4",
    "stylelint": "^13.8.0",
    "typescript": "^4.1.3",
    "umi": "^3.3.7"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=12.16.3"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "license": "MIT"
}
